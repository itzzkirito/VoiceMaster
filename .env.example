# Required
DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_client_id_here
OWNER_ID=your_discord_user_id_here

# Optional
DEVELOPER_IDS=id1,id2,id3
PREFIX=!
GUILD_ID=your_guild_id_here


# Bot Settings
PREFIX=!
NODE_ENV=development
LOG_LEVEL=info

# Database Configuration
# Set DATABASE_TYPE to 'mongodb' (default) or 'postgresql'
DATABASE_TYPE=mongodb

# MongoDB (Default)
MONGODB_URI=mongodb://localhost:27017/discord-bot
# For MongoDB Atlas (cloud):
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/discord-bot

# PostgreSQL (Optional - if DATABASE_TYPE=postgresql)
DATABASE_URL=postgresql://user:password@localhost:5432/discord_bot

# Redis Cache (Optional)
REDIS_URL=redis://localhost:6379



# Spotify API
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Webhooks (Discord Webhook URLs for logging)
NODE_ERROR_LOGS_HOOK=https://discord.com/api/webhooks/your_webhook_url_here
NODE_DESTROY_LOGS_HOOK=https://discord.com/api/webhooks/your_webhook_url_here
NODE_CONNECTION_HOOK=https://discord.com/api/webhooks/your_webhook_url_here
NODE_DISCONNECT_LOGS_HOOK=https://discord.com/api/webhooks/your_webhook_url_here
NODE_RECONNECT_LOGS_HOOK=https://discord.com/api/webhooks/your_webhook_url_here
ERROR_LOGS_HOOK=https://discord.com/api/webhooks/your_webhook_url_here
GUILD_JOIN_LOGS_HOOK=https://discord.com/api/webhooks/your_webhook_url_here
COMMAND_LOGS_HOOK=https://discord.com/api/webhooks/your_webhook_url_here
GUILD_LEAVE_LOGS_HOOK=https://discord.com/api/webhooks/your_webhook_url_here
RUNTIME_LOGS_HOOK=https://discord.com/api/webhooks/your_webhook_url_here
DM_LOGS_HOOK=https://discord.com/api/webhooks/your_webhook_url_here

